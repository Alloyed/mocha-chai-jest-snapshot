{
  "name": "mocha-chai-jest-snapshot",
  "version": "1.1.1",
  "description": "provides snapshot testing like jest",
  "keywords": [
    "mocha",
    "chai",
    "assertion",
    "helper",
    "jest",
    "snapshot",
    "react",
    "test",
    "spec",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mochiya98/mocha-chai-jest-snapshot.git"
  },
  "author": "Yukimasa Funaoka <yukimasafunaoka@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "test": "npm run build && node node_modules/.bin/run-p -l --aggregate-output test:*",
    "test:eslint": "npx eslint --color --cache 'src/**/*.ts' 'test/**/*.ts'",
    "test:mocha": "npx mocha -c -s 10000 -r ts-node/register/transpile-only --file setup.js 'test/**/*.test.ts'",
    "prepack": "npm run build"
  },
  "files": [
    "dist/**",
    "reporters/**",
    "setup.js"
  ],
  "dependencies": {
    "@jest/test-result": "^26.6.2",
    "chalk": "^4.1.0",
    "find-package-json": "^1.2.0",
    "jest-snapshot": "^26.6.2",
    "jest-util": "^26.6.2",
    "slash": "^3.0.0",
    "yargs": "^16.1.1"
  },
  "peerDependencies": {
    "chai": "^4.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/find-package-json": "^1.1.1",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "chai": "^4.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "mocha": "^8.2.1",
    "mock-fs": "^4.13.0",
    "mock-require": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "p-limit": "^3.1.0",
    "prettier": "^2.2.1",
    "pretty-format": "^26.6.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
